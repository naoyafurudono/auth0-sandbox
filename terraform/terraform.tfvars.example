# Auth0 Management API credentials
# これらの値を取得する方法:
# 1. Auth0ダッシュボード → Applications → Applications
# 2. "Auth0 Management API" という名前のアプリケーションを探す
# 3. もしなければ、Applications → APIs → Auth0 Management API → Machine to Machine Applications
#    から新しいアプリケーションを作成して認可する
# 4. そのアプリケーションのClient IDとClient Secretをここに設定

auth0_management_client_id     = "YOUR_MANAGEMENT_CLIENT_ID"
auth0_management_client_secret = "YOUR_MANAGEMENT_CLIENT_SECRET"

# Optional: デフォルト値を上書きする場合
# auth0_domain      = "dev-31tn6c1v6nf53cr3.us.auth0.com"
# app_base_url      = "http://localhost:3000"
# api_identifier    = "https://api.auth0-sandbox.local"
